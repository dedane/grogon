      <v-text-field\n            label='Name'\n            v-model='Name'\n            filled>\n            </v-text-field>\n            <!-- <v-file-input\n            accept='image/png, image/jpeg, image/bmp'\n            append-icon='mdi-camera'\n            label='vehicle Image'\n            v-model='vehicleimage'\n            filled>\n            </v-file-input> -->\n            <v-text-field\n            label='vehicleimage'\n            v-model='VehicleImage'\n            filled>\n            </v-text-field>\n            <v-text-field\n            append-icon='mdi-eye'\n            label='Password'\n            v-model='Password'\n            filled>\n            </v-text-field>\n            <v-text-field\n            append-icon='mdi-phone'\n            label='Phone number'\n            v-model='Phonenumber'\n            filled>\n            </v-text-field>\n            <v-text-field\n            label='Email'\n            v-model='Email'\n            append-icon='mdi-email'\n            filled>\n            </v-text-field>\n            <v-row>\n              <v-col>\n                <v-text-field\n                label='Carmake'\n                v-model='Carmake'\n                append-icon='mdi-car'\n                filled>\n                </v-text-field>\n                <!-- <v-select\n                label='car model'>\n                </v-select> -->\n              </v-col>\n              <v-col>\n                <v-menu\n                  ref=\"menu\"\n                  v-model=\"menu\"\n                  :close-on-content-click=\"false\"\n                  transition=\"scale-transition\"\n                  offset-y\n                  min-width=\"290px\"\n                >\n                <template v-slot:activator=\"{ on, attrs }\">\n                <v-text-field\n                v-model=\"PurchaseDate\"\n                label=\"Model year\"\n                prepend-icon=\"mdi-calendar\"\n                readonly\n                v-bind=\"attrs\"\n                v-on=\"on\"\n                ></v-text-field>\n                </template>\n              <v-date-picker\n                ref=\"picker\"\n                v-model=\"PurchaseDate\"\n                :max=\"new Date().toISOString().substr(0, 10)\"\n                min=\"1950-01-01\"\n                @change=\"save\"\n              ></v-date-picker>\n              </v-menu>\n              </v-col>\n            </v-row>\n            <v-row align='center' justify='center'>\n            <v-btn dark width='400' color='#046e0c'\n            @click=\"submitForm()\">\n              Sign Up\n            </v-btn>\n            </v-row>\n            <v-row class='mt-8' align='center' justify='center'>\n              or join us with\n            </v-row>\n            <v-row align='center'\n            justify='center'>\n              <v-btn class='ma-8'\n              dark\n              color='red'\n              large fab>\n                <v-icon>mdi-google</v-icon>\n              </v-btn>\n              <v-btn class='ma-8'\n              dark\n              color='blue'\n              large fab>\n                <v-icon>mdi-facebook</v-icon>\n              </v-btn>\n            </v-row>\n          </v-form>\n        </v-col>\n      </v-row>\n    </v-container>\n    <Footer />\n  </div>\n</template>\n\n<script>\nimport Header from '@/components/Navbar.vue';\nimport Footer from '@/components/Footer.vue';\n\nexport default {\n  name: 'Car Registration',\n  components: {\n    Header,\n    Footer,\n  },\n  data: () => ({\n    menu: false,\n    Name: '',\n    Email: '',\n    VehicleImage: '',\n    Password: '',\n    Phonenumber: '',\n    PurchaseDate: '',\n    Carmake: '',\n\n  }),\n  watch: {\n    menu(val) {\n      this.$refs.picker.activePicker = 'YEAR';\n      // eslint-disable-next-line no-unused-expressions\n      val && setTimeout(() => ('YEAR'));\n    },\n  },\n  methods: {\n    save(date) {\n      this.$refs.menu.save(date);\n    },\n    submitForm() {\n      this.$store.dispatch('driverRegistration', {\n        Name: this.Name,\n        Email: this.Email,\n        VehicleImage: this.VehicleImage,\n        Password: this.Password,\n        Phonenumber: this.Phonenumber,\n        PurchaseDate: this.PurchaseDate,\n        Carmake: this.Carmake,\n      })\n        .then(() => {\n          this.$router.push('/car login');\n        })\n        .catch((error) => {\n          // eslint-disable-next-line no-console\n          console.log(error);\n        });\n    },\n  },\n};\n</script>\n\n<style>\n\n</style>\n"]}]}
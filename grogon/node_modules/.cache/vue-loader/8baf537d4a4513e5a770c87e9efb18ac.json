odel='Name'\n            filled>\n            </v-text-field>\n            <v-text-field\n            label='Email'\n            v-model='Email'\n            append-icon='mdi-email'\n            filled>\n            </v-text-field>\n            <!-- <v-text-field\n            label='Location'\n            append-icon=\"mdi-map-marker\"\n            v-model='Location'\n            filled>\n            </v-text-field> -->\n            <v-text-field\n            append-icon='mdi-eye'\n            label='Password'\n            v-model='Password'\n            filled>\n            </v-text-field>\n            <v-text-field\n            append-icon='mdi-phone'\n            label='Phone number'\n            v-model='Phonenumber'\n            filled>\n            </v-text-field>\n            <!-- <v-text-field\n            append-icon='mdi-eye'\n            label='Password'\n            v-model='Certificate'\n            filled>\n            </v-text-field> -->\n            <v-file-input\n            prepend-icon='mdi-camera'\n            label='Mechanic Pic'\n            accept='image/*'\n            v-model='MechanicPic'\n            filled >\n            </v-file-input>\n            <!-- <v-file-input\n            accept='image/png, image/jpeg, image/bmp'\n            append-icon='mdi-camera'\n            label='Your Profile Picture'\n            v-model='mechanicPic'\n            filled>\n            </v-file-input>\n            <v-file-input\n            accept='image/png, image/jpeg, image/bmp'\n            append-icon='mdi-certificate'\n            label='Upload your certificate'\n            v-model='certificate'\n            filled>\n            </v-file-input> -->\n            <v-row align='center' justify='center'>\n            <v-btn dark width='400' color='#046e0c'\n            @click=\"submitForm()\">\n              Sign Up\n            </v-btn>\n            </v-row>\n            <v-row class='mt-8' align='center' justify='center'>\n              or join us with\n            </v-row>\n            <v-row align='center'\n            justify='center'\n            class='mb-4'>\n              <v-btn class='ma-8'\n              dark\n              color='red'\n              large fab>\n                <v-icon>mdi-google</v-icon>\n              </v-btn>\n              <v-btn class='ma-8'\n              dark\n              color='blue'\n              large fab>\n                <v-icon>mdi-facebook</v-icon>\n              </v-btn>\n            </v-row>\n          </v-form>\n        </v-col>\n    </v-row>\n    </v-container>\n    <Footer />\n  </div>\n</template>\n\n<script>\nimport Header from '@/components/Navbar.vue';\nimport Footer from '@/components/Footer.vue';\n\nexport default {\n  name: 'MechRegistration',\n  components: {\n    Header,\n    Footer,\n  },\n  data() {\n    return {\n      Name: '',\n      Email: '',\n      /* Location: '', */\n      Password: '',\n      Phonenumber: '',\n      MechanicPic: '',\n      /* Certificate: '', */\n    };\n  },\n  methods: {\n    submitForm() {\n      this.$store.dispatch('mechRegister', {\n        Name: this.Name,\n        Email: this.Email,\n        Password: this.Password,\n        Phonenumber: this.Phonenumber,\n        MechanicPic: this.MechanicPic,\n        /* Certificate: this.Certificate, */\n      })\n        .then(() => {\n          this.$router.push('/mechlogin');\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n};\n\n</script>\n\n<style>\n\n</style>\n"]}]}